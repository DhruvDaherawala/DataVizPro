# DataViz Pro Frontend Environment Variables Example
# Copy this file to .env and modify as needed

# API Configuration
# Use the full URL including protocol (http/https)
VITE_API_URL=data-viz-pro-backend.vercel.app

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_AUTH=false

# App Configuration
VITE_APP_NAME=DataViz Pro
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Data Visualization and Analysis Tool

# Other Settings
VITE_MAX_UPLOAD_SIZE=10485760 # 10MB in bytes

# For Production Environment
# VITE_API_URL=https://your-backend-domain.vercel.app 